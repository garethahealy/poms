<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<suppressions>
    <suppress checks=".*" files=".+[\\\/]generated[\\\/].+\.java"/>
    <suppress checks=".*" files=".+[\\\/]build[\\\/]src[\\\/].+\.java"/>
    <suppress checks=".*" files=".+[\\\/]target[\\\/].+\.java"/>
    <suppress checks=".*" files=".+[\\\/]contrib[\\\/].+\.java"/>

    <!-- Suppress any checks for classes we've had to import, but don't own -->
    <suppress checks=".*" files=".+[\\\/]org[\\\/].+\.java"/>

    <suppress checks=".*" files=".+[\\\/]Application.java"/>
</suppressions>
